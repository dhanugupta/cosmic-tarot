{
  "name": "cosmic-tarot",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf .next node_modules/.cache .swc .next/cache",
    "dev": "npm run clean && NODE_ENV=development next dev",
    "dev:fast": "NODE_ENV=development next dev",
    "dev:safe": "npm run clean && NODE_ENV=development next dev",
    "build": "npm run clean && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "reset": "npm run clean && rm -rf node_modules package-lock.json && npm install"
  },
  "engines": {
    "npm": ">=10.5.0",
    "node": ">=16.12.1"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.0.0"
  }
}
